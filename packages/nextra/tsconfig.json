{
  "compilerOptions": {
    "target": "es5",
    "module": "es2020",
    "declaration": true,
    "noEmit": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "downlevelIteration": true,
    "jsx": "react-jsx",
    "lib": ["ES2022.Object", "ES2021.String", "es2020", "dom"],
    "moduleResolution": "node",
    "types": ["vitest/globals", "webpack-env"],
    "resolveJsonModule": true
  }
}
