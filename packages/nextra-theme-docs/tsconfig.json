{
  "compilerOptions": {
    "target": "es5",
    "module": "es2020",
    "declaration": false,
    "noEmit": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "lib": ["es2020", "DOM", "DOM.Iterable"],
    "types": ["vitest/globals"],
    "resolveJsonModule": true
  }
}
